{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "next build",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATH",
        "DELETE"
      ]
    }
  ]
}